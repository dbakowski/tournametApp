<div class="container">
  <h1>Gracz: <%= player.nickname %></h1>
  <%- partial('../../layouts/partials/crud_view_header.ejs', {
    model: {
      panelName: 'players',
      id: player.id,
      name: player.nickname
    }
  })
  %>
  <table class="table table-dark">
    <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Nick</th>
      <th scope="col">Klasa</th>
      <th scope="col">Team</th>
      <th scope="col">Ranga</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <th scope="row"><%= player.id %></th>
      <td><%= player.nickname %></td>
      <td><%= player.className %></td>
      <td><%= player.team %></td>
      <td><%= player.rank %></td>
    </tr>
    </tbody>
  </table>
</div>
<script src="/js/crud/delete.js"></script>
