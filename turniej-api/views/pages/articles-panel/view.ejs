<div class="container">
  <h1>Artykuł: <%= model.title %></h1>
  <%- partial('../../layouts/partials/crud_view_header.ejs', {
    model: {
      panelName: 'articles',
      id: model.id,
      name: model.title
    }
  })
  %>
  <table class="table table-dark">
    <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Pozycja</th>
      <th scope="col">Tytuł</th>
      <th scope="col">Treść</th>
    </tr>
    </thead>
    <tbody>
    <tr>
      <th scope="row"><%= model.id %></th>
      <td><%= model.position %></td>
      <td><%= model.title %></td>
      <td><%= model.body %></td>
    </tr>
    </tbody>
  </table>
</div>
<script src="/js/crud/delete.js"></script>
