<div class="container">
  <h1>Utwórz Artykuł:</h1>
  <div class="d-flex justify-content-end" id="button-container">
    <a href="/articles-panel">
      <button type="button" class="btn btn-success">Wróc do listy</button>
    </a>
  </div>
  <form action="/articles-panel/create" method="post">
    <div class="form-group">
      <label for="position">Pozycja: </label>
      <input type="text" class="form-control" name="position" id="position">
    </div>
    <div class="form-group">
      <label for="title">Tytuł: </label>
      <input type="text" class="form-control" name="title" id="title">
    </div>
    <div class="form-group">
      <label for="body">Treść: </label>
      <textarea name="body" id="body" cols="30" rows="10"></textarea>
    </div>
    <div class="form-group">
      <button id="submit" class="btn btn-primary" type="submit">Zapisz</button>
    </div>
  </form>
</div>
<script src="/js/articles-panel/validateForm.js"></script>
