<div class="container">
  <h1>Lista artykułów</h1>
  <div class="d-flex justify-content-end" id="button-container">
    <a class="pull-right" href="/articles-panel/create">
      <button type="button" class="btn btn-outline-primary">Utwórz</button>
    </a>
  </div>
  <table class="table table-dark">
    <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Pozycja</th>
      <th scope="col">Tytuł</th>
      <th scope="col">Akcje</th>
    </tr>
    </thead>
    <tbody id="records">
    <% models.forEach(function (model) { %>
      <tr>
        <th scope="row"><%= model.id %></th>
        <td><%= model.position %></td>
        <td><%= model.title %></td>
        <%- partial('../../layouts/partials/crud_index_buttons.ejs', {
          model: {
            panelName: 'articles',
            id: model.id,
            name: model.title
          }
        })
        %>
      </tr>
    <% }) %>
    </tbody>
  </table>
</div>
<script src="/js/crud/delete.js"></script>
